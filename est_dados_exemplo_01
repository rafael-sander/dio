programa
{
	funcao inicio()
	{
		inteiro vetor[] = {1,3,5,7,9}
		inteiro numero
		logico achou = falso
		
		escreva("Qual número deseja procurar?\n")
		leia(numero)

		para(inteiro posicao = 0; posicao < 5; posicao++)
		{
		    se (vetor[posicao] == numero)
		    {
		        escreva("Número encontrado na posição: ", posicao, "\n")
		        achou = verdadeiro
		    }
		}
		se (achou == falso)
		{
		    escreva("Número não está no vetor", "\n")
		}
	}
}

//Função do algoritmo: exemplo de utilização de vetor
//Autor: Rafael Sander

programa
{
	funcao inicio()
	{
	    inteiro contador = 0
		cadeia frutas[4]
		
		frutas[0] = "Maçã"
		frutas[1] = "Pera"
		frutas[2] = "Uva"
		frutas[3] = "Jaca"
		
		faca
		{
		    escreva(frutas[contador] + "\n")
		    contador++
		}
		enquanto (contador <= 3)
	}
}
